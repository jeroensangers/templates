/* CSS Resets */
html,body,div,span,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,abbr,address,cite,code,del,dfn,em,img,ins,q,small,strong,sub,sup,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,table,caption,tbody,tfoot,thead,tr,th,td{border:0;margin:0;padding:0}article,aside,figure,figure img,figcaption,hgroup,footer,header,nav,section,video,object{display:block}a img{border:0}figure{position:relative}figure img{width:100%}

/* ==================================================================================================================== */
/* ! The 1140px Grid V2 by Andy Taylor \ http://cssgrid.net \ http://www.twitter.com/andytlr \ http://www.andytlr.com   */
/* ==================================================================================================================== */
.container {
	padding-left: 20px;
	padding-right: 20px;
}
.row {
	width: 100%;
	max-width: 1140px;
	min-width: 755px;
	margin: 0 auto;
	overflow: hidden;
}
.onecol, .twocol, .threecol, .fourcol, .fivecol, .sixcol, .sevencol, .eightcol, .ninecol, .tencol, .elevencol {
	margin-right: 3.8%;
	float: left;
	min-height: 1px;
}
.row .onecol {
	width: 4.85%;
}
.row .twocol {
	width: 13.45%;
}
.row .threecol {
	width: 22.05%;
}
.row .fourcol {
	width: 30.75%;
}
.row .fivecol {
	width: 39.45%;
}
.row .sixcol {
	width: 48%;
}
.row .sevencol {
	width: 56.75%;
}
.row .eightcol {
	width: 65.4%;
}
.row .ninecol {
	width: 74.05%;
}
.row .tencol {
	width: 82.7%;
}
.row .elevencol {
	width: 91.35%;
}
.row .twelvecol {
	width: 100%;
	float: left;
}
.last {
	margin-right: 0px;
}
img, object, embed {
	max-width: 100%;
}
img {
	height: auto;
}

/* Smaller screens */

@media only screen and (max-width: 1023px) {
	body {
		font-size: 0.8em;
		line-height: 1.5em;
	}
}

/* Mobile */

@media handheld, only screen and (max-width: 767px) {
	body {
		font-size: 16px;
		-webkit-text-size-adjust: none;
	}

	.row, body, .container {
		width: 100%;
		min-width: 0;
		margin-left: 0px;
		margin-right: 0px;
		padding-left: 0px;
		padding-right: 0px;
	}

	.row .onecol, .row .twocol, .row .threecol, .row .fourcol, .row .fivecol, .row .sixcol, .row .sevencol, .row .eightcol, .row .ninecol, .row .tencol, .row .elevencol, .row .twelvecol {
		width: auto;
		float: none;
		margin-left: 0px;
		margin-right: 0px;
		padding-left: 20px;
		padding-right: 20px;
	}
}

/* ============================== */
/* ! Layout for desktop version   */
/* ============================== */
body {
  background: #f3f5f6 url(/img/bg.png) 0 0;
  color: #262621;
  font: normal normal 300 100%/1.5 "Open Sans", "Gill Sans","Gill Sans MT",Calibri,Sans-Serif;
}
h1, h2, h3, h4, h5, h6 {
  padding: 10px 16px 24px;
  padding: 0.625rem 1rem 1.5rem;
  color: #74489D;
  font-weight: 400;
  font-family: 'Bree Serif',"Arial Black",Gadget,sans-serif;
  line-height: 1.1;
  text-rendering: optimizeLegibility;
}
p {
  margin: 0 16px 16px;
  margin: 0 1rem 1rem;
  text-align: justify;
}
a,
a:visited  {
  color: #AD6CEB;
  text-decoration: none;
}
a:hover,
a:active {
  text-decoration: underline;
}
a img {
  border: none;
}
abbr {
	font-variant: small-caps;
}
blockquote {
  overflow: hidden;
  margin-bottom: 10px;
  margin-bottom: 0.625rem;
  margin-left: 16px;
  margin-left: 1rem;
  padding-left: 50px;
  background: url(/img/quotes.gif) no-repeat scroll 10px 5px transparent;
  color: #57564a;
  font-size: 18px;
  font-size: 1.125rem;
  font-style: italic;
}
blockquote p {
  margin-left: 0;
}
form {
  padding: 16px;
  padding: 1rem;
}
ul {
  margin: 0 32px 32px;
  margin: 0 2rem 2rem;
}
ol {
  margin: 0 32px 32px;
  margin: 0 2rem 2rem;
}
h1 {
  font-size: 32px;
  font-size: 2rem;
}
h2 {
  font-size: 24px;
  font-size: 1.5rem;
}
h3 {
  font-size: 18px;
  font-size: 1.125rem;
}
strong,
b {
  font-weight: 600;
}
q {
  font-style: italic;
}
pre {
  width: 100%;
  padding-bottom: 8px;
  padding-bottom: 0.5rem;
  margin: 0 16px 16px;
  margin: 0 1rem 1rem;
  overflow: auto;
  border: 1px solid #808080;
}
code {
  font: .9em Monaco, monospace, Courier, "Courier New";
}
input,
textarea {
  padding: 4px;
  font-size: 16px;
  font-size: 1rem;
}
form label {
  cursor: pointer;
}
.page {
  font-size: 18px;
  font-size: 1.125rem;
}
aside h1,
.offers h1,
.entries h1 {
  font-size: 24px;
  font-size: 1.5rem;
}
aside h2 {
  font-size: 18px;
  font-size: 1.125rem;
}
h1 a,
h1 a:visited {
  color: #ACB911;
}
aside ul,
article ul {
  list-style: none;
}
aside ul {
  margin: 0 32px 32px 0;
  margin: 0 2rem 2rem 0;
}
aside ul li,
article ul li {
  padding-left: 15px;
  background: url(/img/bullet.gif) no-repeat 0 6px;
  text-align: justify;
}
#header {
  margin-bottom: 16px;
  margin-bottom: 1rem;
  border-bottom: 1px solid #68831C;
  background: #ACB911 url(/img/header-bg.png) 0 0;
}
#header h1 {
  margin-top: 16px;
  text-indent: 100%;
	white-space: nowrap;
	overflow: hidden;
	background: url(/img/logo.png) no-repeat;
	background-size: 350px 60px;
}
nav {
  margin-top: 42px;
  margin-top: 2.625rem;
  text-transform: lowercase;
}
nav ul {
  float: right;
  list-style: none;
}
nav li {
  float: left;
  padding: 0 0 0 24px;
  padding: 0 0 0 1.5rem;
}
nav li a,
nav li a:visited {
  color: #FFF;
  text-shadow: #0E161B 2px 2px 3px;
  font-size: 18px;
  font-size: 1.125rem;
}
nav li a:hover,
nav li a:active {
  color: #EED9F9;
}
.pitch p {
  font-size: 18px;
  font-size: 1.125rem;
}
form#mc-embedded-subscribe-form {
  margin-right: auto;
  margin-bottom: 16px;
  margin-bottom: 1rem;
  margin-left: auto;
  max-width: 350px;
  width: 90%;
  text-align: center;
}
.pitch form,
.page form,
form.wufoo {
  clear: both;
  border: 1px solid #868670;
  border-radius: 5px;
  background: #bababa url(/img/bg-grey.png) 0 0;
  box-shadow: #262621 2px 2px 6px;
}
.offers {
  margin-top: 32px;
  margin-top: 2rem;
  border-top: 1px solid #090909;  
  background: #090909 url(/img/bg-offers.gif) repeat-x;
  color: #b1b094;
}
.offers h1 {
  color: #fff;
}
.offers img {
  padding: 5px;
  background-color: #fff;
}
.entries {
  margin-top: 32px;
  margin-top: 2rem;
}
.thumbnail {
  float: left;
  margin: 0 24px 10px 16px;
  margin: 0 1.5rem 0.625rem 1rem;
  padding: 4px;
  border: 1px solid #ccc;
  background: #fff;
}
.feature {
  float: left;
  margin: 0 15px 0 0;
  max-width: 230px;
  max-height: 200px;
  width: 230px;
  height: 200px;
  border: 5px solid #f3f5f6;
}
.excerpt {
  overflow: auto;
  padding-top: 10px;
  border-top: 1px solid #c5b7d4;
}
.featured {
  overflow: auto;
  padding: 10px;
  border-radius: 5px;
  background: #bababa url(/img/bg-grey.png) 0 0;
  font-size: 18px;
  font-size: 1.125rem;
}
.featured h1 {
  padding: 0 18px 18px;
  padding: 0 1.125rem 1.125rem;
}
.featured h1 a,
.featured h1 a:visited {
  color: #74489D;
} 
.featured a,
.featured a:visited {
  color: #f3f5f6;
}
h1.blog {
  padding: 24px 16px 10px;
  padding: 1.5rem 1rem 0.625rem;
}
iframe {
  margin: 0 16px 16px;
  margin: 0 1rem 1rem;
}
aside iframe {
  margin: 0;
}
.byline {
  font-style: italic;
  font-size: 14px;
  font-size: 0.875rem;
}
.author-profile img {
  float: left;
  margin: 0 10px 10px 0;
  margin: 0 0.625rem 0.625rem 0;
  border: 4px solid #f3f5f6;
}
.author-profile h2{
  padding-top: 0;
  padding-bottom: 0;
  font-style: normal;
}
#comment-greeting {
  margin: 0 16px 16px;
  margin: 0 1rem 1rem;
}
#comments-open-data div,
form #comment-form-reply,
form #comment-form-subscribe {
  clear: left;
  padding: 5px 0 5px 0;
}
#comments-open-data label {
  display: block;
  float: left;
  padding-top: 4px;
  padding-right: 10px;
  width: 100px;
  text-align: right;
  font-weight: 600;
}
#comments-open-data input {
  float: left;
}
#comments-open-data #comment-form-remember-me,
form #comment-form-reply,
form #comment-form-subscribe {
  padding-left: 105px;
}
#comments-open-data #comment-form-remember-me label,
#comment-form-reply label,
#comment-form-subscribe label {
  position: relative;
  top: -3px;
  width: 250px;
  text-align: left;
  font-weight: 600;
}
div.aprendemas {
  float: left;
  width: 125px;
  height: 125px;
  line-height:1.125;
}
.entries h1 {
  padding: 0 16px 16px;
  padding: 0 1rem 1rem;
}
h1.blog-15 {
  padding: 0 0 8px 40px;
  padding: 0 0 0.5rem 2.5rem;
  background: url(/img/link.png) no-repeat 16px top;
  font-size: 16px;
  font-size: 1rem;
  font-family: "Open Sans", "Gill Sans","Gill Sans MT",Calibri,Sans-Serif;
  font-weight: 300;
}
.factbox,
.author-profile {
  margin: 0 16px 16px;
  margin: 0 1rem 1rem;
  padding: 10px;
  border: 1px solid #868670;
  border-radius: 5px;
  background: #bababa url(/img/bg-grey.png) 0 0;
  box-shadow: rgb(51,51,51) 2px 2px 10px;
  text-align: justify;
  font-style: italic;
  font-size: 18px;
  font-size: 1.125rem;
}
div.r {
  float: right;
}
div.l {
  float: left;
}
.factbox img {
  border: 4px solid #f3f5f6;
}
table.data {
  margin: 0 16px 16px;
  margin: 0 1rem 1rem;
}
.data th {
  padding: 6px 6px 6px 12px;
  border-top: 1px solid #C1DAD7;
  border-right: 1px solid #C1DAD7;
  border-bottom: 1px solid #C1DAD7;
  background: #CAE8EA url(/img/table-header.jpg) no-repeat;
  color: #6D929B;
  text-align: left;
  text-transform: uppercase;
  letter-spacing: 2px;
  font: 600 11px "Open Sans","Gill Sans","Gill Sans MT",Calibri,Sans-Serif;
}
.data th.nobg {
  border-top: 0;
  border-right: 1px solid #C1DAD7;
  border-left: 0;
  background: none;
}
tr.odd  th {
  border-top: 0;    
  border-left: 1px solid #C1DAD7;
  background: #fff url(/img/table-bullet1.gif) no-repeat;
  font: 600 11px "Open Sans","Gill Sans","Gill Sans MT",Calibri,Sans-Serif;
}
tr.even th {
  border-top: 0;
  border-left: 1px solid #C1DAD7;
  background: #f5fafa url(/img/table-bullet2.gif) no-repeat;
  color: #B4AA9D;
  font: 600 11px "Open Sans","Gill Sans","Gill Sans MT",Calibri,Sans-Serif;
}
.data td {
  padding: 6px 6px 6px 12px;
  border-right: 1px solid #C1DAD7;
  border-bottom: 1px solid #C1DAD7;
  background: #fff;
  color: #6D929B;
  text-align: center;
  font: 600 11px "Open Sans","Gill Sans","Gill Sans MT",Calibri,Sans-Serif;
}
tr.even td {
  background: #F5FAFA;
  color: #B4AA9D;
}
div.iframe {
  position: relative;
  margin: 0 16px 16px;
  margin: 0 1rem 1rem;
  height: 0;
}
div.ratio169 {
  padding-bottom: 56.25%;
}
div.ratio43 {
  padding-bottom: 75%;
}
div.iframe iframe {
  position: absolute;
  top: 0;
  left: 0;
  margin: 0;
  width: 100%;
  height: 100%;
}
p.link {
  font-size: 14px;
  font-size: 0.875rem;
  font-style: italic; 
}
section.comments {
  border-top: 1px solid #c5b7d4;
}
ol.comment-list {
  margin: 0;
  list-style: none;
}
ol.comment-list ol.comment-list {
  margin-left: 32px;
  margin-left: 2rem;
}
li.comment {
  overflow: hidden;
  margin: 0 0 16px 0;
  margin: 0 0 1rem 0;
  padding: 16px 16px 0 16px;
  padding: 1rem 1rem 0 1rem;
  border-top: 1px solid #c5b7d4;
  border-bottom: 1px solid #c5b7d4;
  background: #F7F7F7;
}
li.entry-author {
  background: #CAE8EA;
}
.meta,
.replybutton {
  margin: 0 16px 16px;
  margin: 0 1rem 1rem;
}
img.gravatar {
  float: right;
  margin-bottom: 16px;
  margin-bottom: 1rem;
  margin-left: 16px;
  margin-left: 1rem;
  border: 4px solid #FFF;
}
.sharebutton {
  float: left;
  margin: 0 5px;
  margin-bottom: 16px;
  margin-bottom: 1rem;
}
.subscription {
  clear: both;
  border-radius: 5px;
  background: #ACB911 url(/img/header-bg.png) 0 0;
  color: #fff;
  font-size: 14px;
  font-size: 0.875rem;
}
.subscription h1 {
  color: #fff;
}
.subscription img {
  display: block;
  margin: 0 auto;
}
footer {
  margin-top: 32px;
  margin-top: 2rem;
  padding: 32px 20px;
  padding: 2rem 1.25rem;
  border-top: 1px solid #68831C;
  background: #ACB911 url(/img/header-bg.png) 0 0;
  text-align: center;
}
footer a,
footer a:visited {
  color: #FFF;
}
footer a:hover,
footer a:active {
  color: #EED9F9;
}
footer row {
	margin: 20px auto;
}
.search form {
  margin-right: auto;
  margin-left: auto;
  width: 300px;
  text-align: center;
}
#search-submit {
  visibility: hidden;
}

