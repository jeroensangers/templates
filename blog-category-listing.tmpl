<mt:SetVarBlock name="title">Posts sobre <$MTArchiveTitle$> en <$mt:BlogName encode_html="1"$></mt:SetVarBlock>
<mt:SetVarBlock name="html_head">
  <meta name="description" content="Archivo: Todos los posts sobre <$MTArchiveTitle$> en <$mt:BlogName encode_html="1"$>" />
  <link rel="canonical" href="<$mt:ArchiveLink$>"/>
</mt:SetVarBlock>
<mt:SetVarBlock name="NoSocial">True</mt:SetVarBlock>
<$mt:Include module="Header" parent="1"$>

<div class="container">
  <div class="row">
    <div class="eightcol">
      <h1>Posts sobre <$mt:ArchiveTitle$></h1>
<mt:IfNonEmpty tag="CategoryDescription"> 
      <p><$mt:CategoryDescription$></p>
</mt:IfNonEmpty>
      <section class="entries">
<mt:Entries>
        <$mt:Include module="Entry" parent="1"$>
</mt:Entries>
      </section>
    </div>
    <aside class="fourcol last">
<mt:BlogParentWebsite> 
  <$mt:WebsiteID setvar="website_id"$> 
</mt:BlogParentWebsite> 
      <$mt:WidgetSet name="Blog widgets" blog_id="$website_id"$>
    </aside>
  </div>
</div>

<$mt:Include module="Footer" parent="1"$>